
{
	"useTabs": true,
	"singleQuote": true,
	"trailingComma": "none",
	"semi": false,
	"printWidth": 100,
	"plugins": ["prettier-plugin-tailwindcss", "prettier-plugin-astro"],
	"bracketSameLine": true,
	"overrides": [
		{
			"files": "*.astro",
			"options": {
				"parser": "astro"
			}
		}
	],
	"pluginSearchDirs": false
}
